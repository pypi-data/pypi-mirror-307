[run]
source_pkgs = nti.wref
relative_files = True


[report]
# Coverage is run on Linux under cPython 2 and 3,
# exclude branches that are windows, pypy
# specific
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if PYPY:
    if sys.platform == 'win32':
    if mswindows:
    if is_windows:
fail_under = 99.0
precision = 2

