    "name": "Haymaker Cocktail",
    "prepTime": "PT10M",
    "cookTime": "PT0S",
    "totalTime": "PT10M",
    "recipeIngredient": [
        "1 tsp. lime zest, plus 1/4 ounce lime juice, divided",
        "2 tbsp. sugar",
        "3/4 oz. bourbon",
        "3/4 oz. sweet vermouth",
        "1/4 oz. Cointreau"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Stir together lime zest and sugar on a small plate. Fill a second small plate with water. Dip rim of an old-fashioned glass in water, then in sugar mixture.&nbsp;"
        },
        {
            "@type": "HowToStep",
            "text": "Fill a cocktail shaker with bourbon, vermouth, Cointreau, lime juice, and ice. Shake until chilled, 10 to 15 seconds. Strain into prepared glass over ice."
        }
    ],
