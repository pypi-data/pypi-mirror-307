    "recipeIngredient": [
        "250g butter",
        "250g soft brown sugar",
        "175g golden syrup",
        "425g oats",
        "200g cherries , pitted and halved",
        "50g coconut flakes"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Heat oven to 180C/160C fan/gas 4 and line a 20 x 30cm baking tin with parchment. Melt the butter, sugar and syrup in a large pan over a medium heat. Once melted, stir in the oats, coating them well."
        },
        {
            "@type": "HowToStep",
            "text": "Tip ¾ of the mix into the tin and scatter the cherries over the top. Combine the remaining oat mix with the coconut flakes and tip on top of the cherries, making sure a few poke out. Bake for 30 mins, then remove from the oven and allow to cool before slicing into 12 flapjacks."
        }
    ],
    "recipeYield": 12,
    "totalTime": "PT45M"
