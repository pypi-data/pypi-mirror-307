    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Heat oven to 375°F. In large bowl, stir all ingredients until soft dough forms. Drop dough by rounded teaspoonfuls 2 inches apart onto ungreased cookie sheet."
        },
        {
            "@type": "HowToStep",
            "text": "Bake 10 to 12 minutes or until edges are golden brown. Cool 1 minute; remove from cookie sheet. Cool completely."
        }
    ],
