    "recipeIngredient": [
        "1 tablespoon extra-virgin olive oil",
        "1 small onion, diced",
        "2 cloves garlic, minced",
        "2 cans (28 ounces each) whole tomatoes",
        "0.25 teaspoon dried oregano",
        "Kosher salt and freshly ground pepper"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "In a medium saucepan, heat oil over medium. Cook onion and garlic, stirring frequently, until translucent, 2 to 4 minutes. Crush tomatoes into pan; add oregano. Simmer, stirring occasionally, until thickened, 15 minutes. Season with salt and pepper."
        }
    ],
