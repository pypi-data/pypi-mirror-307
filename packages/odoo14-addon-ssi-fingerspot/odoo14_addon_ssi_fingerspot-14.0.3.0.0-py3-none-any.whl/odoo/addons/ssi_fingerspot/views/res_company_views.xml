<odoo>
<record id="res_company_view_form" model="ir.ui.view">
    <field name="name">res.company - Klik Pajak Form</field>
    <field name="model">res.company</field>
    <field name="inherit_id" ref="base.view_company_form" />
    <field name="arch" type="xml">
        <data>
            <xpath expr="//form//notebook" position="inside">
                <page name="fingerspot" string="Fingerspot">
                    <group name="fingerspot_1" colspan="4" col="2">
                        <group name="fingerspot_1_1" colspan="1" col="2">
                            <field name="fingerspot_backend_id" />
                        </group>
                    </group>
                </page>
            </xpath>
        </data>
    </field>
</record>
</odoo>
